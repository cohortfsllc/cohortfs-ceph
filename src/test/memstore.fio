######################################################################
# Example test for the external fio ioengine for MemStore.
# 
# Runs a 4k random write test against a MemStore configuration.
#
######################################################################
[global]
#logging
#write_iops_log=write_iops_log
#write_bw_log=write_bw_log
#write_lat_log=write_lat_log
ioengine=./.libs/libfio_ceph_filestore.so
invalidate=0	# mandatory
#rw=randwrite
#bs=4k

[filestore]
iodepth=1
# create a memstore
objectstore=memstore
osdpath=/mnt/fio_ceph_filestore.XXXXXXX
